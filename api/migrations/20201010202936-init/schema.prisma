datasource db {
  provider = "postgresql"
  url = "***"
}

generator db {
  provider = "go run github.com/prisma/prisma-client-go"
}

model User {
  id       String @id @default(uuid())
  email    String
  password String
}

model Event {
  id       String @id @default(uuid())
  title    String
  img      String
  date     String
  type     String
  max      Int
  hostName String
  hostURL  String
  link     String
}

model Attends {
  id      String @id @default(uuid())
  eventId String
  userId  String
}
